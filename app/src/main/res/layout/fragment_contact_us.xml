            <TextView android:textSize="15sp" android:textColor="@color/category_defaullt" android:gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_10" android:text="@string/mail_id" style="@style/tv"/>
            <EditText android:textSize="15sp" android:textColor="@color/category_defaullt" android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/etMailId" android:background="@drawable/bg_rec" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_10" android:hint="@string/hint_mail_id" style="@style/et"/>
            <TextView android:textSize="15sp" android:textColor="@color/category_defaullt" android:gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_10" android:text="@string/phone_number" style="@style/tv"/>
            <EditText android:textSize="15sp" android:textColor="@color/category_defaullt" android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/etMobileNumber" android:background="@drawable/bg_rec" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_10" android:hint="@string/hint_phone_number" android:inputType="phone" style="@style/et"/>
            <TextView android:textSize="15sp" android:textColor="@color/category_defaullt" android:gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_10" android:text="@string/message" style="@style/tv"/>
            <EditText android:textSize="15sp" android:textColor="@color/category_defaullt" android:gravity="start" android:id="@+id/etMessage" android:background="@drawable/bg_rec" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_10" android:hint="@string/hint_message" android:maxLines="8" android:lines="4" style="@style/et"/>
