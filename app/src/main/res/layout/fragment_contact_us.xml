                    <TextView android:textSize="13sp" android:textColor="@color/category_defaullt" android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/tvEmail" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_10" android:layout_toRightOf="@+id/icStoreMail" android:layout_centerVertical="true" android:layout_marginStart="@dimen/dp_10" android:layout_toEndOf="@+id/icStoreMail" style="@style/tv"/>
                </RelativeLayout>
                <RelativeLayout android:id="@+id/rlSkype" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_10">
                    <ImageView android:id="@+id/icStoreSkype" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_store_sykpe" android:layout_centerVertical="true" android:contentDescription="@string/app_name"/>
                    <TextView android:textSize="13sp" android:textColor="@color/category_defaullt" android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/tvSkype" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_10" android:layout_toRightOf="@+id/icStoreSkype" android:layout_centerVertical="true" android:layout_marginStart="@dimen/dp_10" android:layout_toEndOf="@+id/icStoreSkype" style="@style/tv"/>
                </RelativeLayout>
                <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_10">
                    <ImageView android:id="@+id/icStoreWeb" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_store_web" android:layout_centerVertical="true" android:contentDescription="@string/app_name"/>
                    <TextView android:textSize="13sp" android:textColor="@color/category_defaullt" android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/tvWeb" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_10" android:layout_toRightOf="@+id/icStoreWeb" android:layout_centerVertical="true" android:layout_marginStart="@dimen/dp_10" android:layout_toEndOf="@+id/icStoreWeb" style="@style/tv"/>
