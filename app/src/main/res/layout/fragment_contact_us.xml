            <LinearLayout android:orientation="vertical" android:id="@+id/llInfo" android:visibility="invisible" android:layout_width="match_parent" android:layout_height="wrap_content">
                <RelativeLayout android:id="@+id/rlCall1" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_16">
                    <ImageView android:id="@+id/icStoreCall1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_store_call" android:layout_centerVertical="true" android:contentDescription="@string/app_name"/>
                    <TextView android:textSize="13sp" android:textColor="@color/category_defaullt" android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/tvCall1" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_10" android:text="+919790153222," android:layout_toRightOf="@+id/icStoreCall1" android:layout_centerVertical="true" android:layout_marginStart="@dimen/dp_10" android:layout_toEndOf="@+id/icStoreCall1" style="@style/tv"/>
                </RelativeLayout>
                <RelativeLayout android:id="@+id/rlCall2" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_16">
                    <ImageView android:id="@+id/icStoreCall2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_store_call" android:layout_centerVertical="true" android:contentDescription="@string/app_name"/>
                    <TextView android:textSize="13sp" android:textColor="@color/category_defaullt" android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/tvCall2" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dp_10" android:text="+919790153222," android:layout_toRightOf="@+id/icStoreCall2" android:layout_centerVertical="true" android:layout_marginStart="@dimen/dp_10" android:layout_toEndOf="@+id/icStoreCall2" style="@style/tv"/>
                </RelativeLayout>
                <RelativeLayout android:id="@+id/rlMail" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_10">
